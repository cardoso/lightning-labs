{
  "compilerOptions": {
    "allowJs": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "outDir": "dist/browser",
    "declaration": true,
    "declarationDir": "dist/browser",
    "skipLibCheck": true,
    "module": "NodeNext",
    "moduleResolution": "NodeNext"
  },
  "include": ["src/browser/**/*"],
  "exclude": ["node_modules", "dist"]
}
