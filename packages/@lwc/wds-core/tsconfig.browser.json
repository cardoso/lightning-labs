{
  "compilerOptions": {
    "allowJs": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "outDir": "dist/browser",
    "declaration": true,
    "declarationDir": "dist/browser",
    "target": "ES2015",
    "skipLibCheck": true,
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "esModuleInterop": true
  },
  "include": ["src/browser/**/*"]
}
